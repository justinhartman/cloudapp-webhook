#!/usr/bin/env python3
# -*- coding: UTF-8 -*-
"""
Config

Run the server-side setup scripts.
"""
import subprocess


def process(command, arg):
    return subprocess.run(
        [command, arg],
        check=True,
        text=True
    )


print('ðŸŸ¢ Running the Python setup config subprocess command...')
process('cd', '/app/')
process('bash', './bin/setup')
print('ðŸŸ¢ Completed the subprocess command!')
